<?xml version="1.0" encoding="UTF-8"?>  
    <!DOCTYPE generatorConfiguration  
            PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
            "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">  
    <generatorConfiguration>  
          
       <!-- 数据库连接地址 -->
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<property name="javaFileEncoding" value="UTF-8" />
		<!-- 格式化java代码 -->
		<property name="javaFormatter"
			value="org.mybatis.generator.api.dom.DefaultJavaFormatter" />
		<!-- 格式化XML代码 -->
		<property name="xmlFormatter"
			value="org.mybatis.generator.api.dom.DefaultXmlFormatter" />
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />
		<!-- 增加Models ToStirng方法 -->
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
		<!-- 增加Models Serializable实现 -->
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<!-- 分页插件 -->
	 	<plugin type="com.ower.dsyz.common.mybatis.generator.ext.plugins.MysqlPaginationPlugin"></plugin>

		<!--数据库表注释生成 -->
		<commentGenerator
			type="com.ower.dsyz.common.mybatis.generator.ext.CustomeCommentGenerator">
			<property name="javaFileEncoding" value="UTF-8" />
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="false" />
			<property name="suppressDate" value="false" />
		</commentGenerator>


		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/gateway" userId="root"
			password="foresee"><!-- 123456 -->
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- generate Model -->
		<javaModelGenerator targetPackage="com.ower.dsyz.gateway.auto.model"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- generate xml -->
		<sqlMapGenerator targetPackage="com.ower.dsyz.gateway.auto.mapper"
			targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- generate Mapper -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.ower.dsyz.gateway.auto.dao" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- <table schema="" tableName="t_admin_function" domainObjectName="AdminFunction"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
		<table schema="" tableName="t_admin_role" domainObjectName="AdminRole"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>-->
		<table schema="" tableName="t_gateway_auth_url" domainObjectName="GatewayAuthUrl"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table> 
		<table schema="" tableName="t_gateway_excute_type" domainObjectName="GatewayExcuteType"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
		</context>
    </generatorConfiguration>  